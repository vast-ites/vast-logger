# .env.example - Template for environment variables
# Copy this to .env and fill in your values

# Database Credentials (CHANGE THESE IN PRODUCTION!)
CLICKHOUSE_PASSWORD=CHANGE_ME_$(openssl rand -base64 32)
INFLUX_TOKEN=CHANGE_ME_$(openssl rand -base64 32)

# ClickHouse Connection
CLICKHOUSE_DSN=

# InfluxDB Connection
INFLUX_URL=http://localhost:8086

# Security
AUTH_ENABLED=true
ALLOW_ANONYMOUS_ACCESS=false

# Admin Credentials (CHANGE THESE!)
ADMIN_PASSWORD=admin123
SYSTEM_API_KEY=CHANGE_ME

# MFA Settings
MFA_ENABLED=false
MFA_SECRET=

# Agent Secrets
# Add your agent IDs and secrets here
AGENT_SECRETS={}