# DataVAST Environment Configuration

# Database Credentials
CLICKHOUSE_PASSWORD=securepass
INFLUX_TOKEN=my-super-secret-auth-token

# ClickHouse Connection
CLICKHOUSE_DSN=tcp://datavast:securepass@localhost:9000/datavast

# InfluxDB Connection
INFLUX_URL=http://localhost:8086

# Security - Set to true to enable authentication
AUTH_ENABLED=true
ALLOW_ANONYMOUS_ACCESS=false

# Admin Credentials
ADMIN_PASSWORD=admin123
SYSTEM_API_KEY=default-api-key

# MFA Settings
MFA_ENABLED=false
MFA_SECRET=

# Agent Secrets
AGENT_SECRETS={}
